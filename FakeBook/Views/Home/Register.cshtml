@{
    ViewBag.Title = "Register";
}
 
<div class="row">

    <div class=" container">
        <div class=" row div-form ">

            <h2 class="form-header">Register Form</h2>

            <form id="Register ">
                <div class="form-group   ">
                    <input class="form-input" autoFocus type="text" placeholder="First Name" />

                </div>
                <div class="form-group ">
                    <input class="form-input" autoFocus type="text" placeholder="Last Name" />
                </div>

                <div class="form-group   ">
                    <input class="form-input" autoFocus type="email" placeholder="E-mail"/>

                </div>
                <div class="form-group   ">
                    <input class="form-input" autoFocus type="password" placeholder="Password"/>
                </div>
                <div class="form-group   ">
                    <input class="form-input" autoFocus type="password" placeholder="Confirm Password"/>
                </div>

                <div><button class="btn btn-primary-form ">Register</button></div>
               
            </form>

        </div>



    </div>
</div>